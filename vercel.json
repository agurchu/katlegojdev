{
  "buildCommand": "cd client && npm install --force && ./node_modules/vite/bin/vite.js build",
  "outputDirectory": "client/dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/backend/(.*)",
      "destination": "/backend/index.js"
    },
    { "source": "/(.*)", "destination": "/" }
  ]
}
